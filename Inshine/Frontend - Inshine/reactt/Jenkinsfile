pipeline {
    
    agent any 
        
    stages {
        
        stage('Checkout'){
           steps {
                git credentialsId: '541c943b-b2fd-43a4-ac98-6d5c41dd253c', 
                url: 'https://github.com/atulpatil22/kubernetes_deploy/',
                branch: 'main'
           }
        }

        stage('Build Docker'){
            steps{
                script{
                    sh '''
                    echo 'Buid Docker Image'
                    docker build -t atulpatil22/jenkins_docker .
                    '''
                }
            }
        }


    }
}
